{
  "name": "lanyon",
  "version": "0.0.101",
  "description": "Jekyll without the Hassle",
  "main": "lib/config.js",
  "engines": {
    "node": ">=0.12.18"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "src/"
    ]
  },
  "files": [
    "example",
    "Gemfile.lock",
    "lib",
    "Makefile",
    "scripts",
    "src"
  ],
  "bin": {
    "lanyon": "lib/cli.js"
  },
  "scripts": {
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib",
    "commit:gemlockfile": "git commit Gemfile.lock -m 'Save updated Gemfile.lock' || true",
    "push": "git push && git push --tags",
    "container:rebuild": "env LANYON_RESET=1 LANYON_UPDATE_GEM_LOCKFILE=1 DOCKER_RESET=1 DOCKER_BUILD=1 DEBUG=*:* LANYON_ONLY=docker LANYON_PROJECT=$(pwd)/example node lib/cli.js install",
    "container:build": "env LANYON_RESET=1 LANYON_UPDATE_GEM_LOCKFILE=1 DOCKER_BUILD=1 DEBUG=*:* LANYON_ONLY=docker LANYON_PROJECT=$(pwd)/example node lib/cli.js install",
    "container:connect": "node lib/cli.js container:connect",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && npm-run-all build container:build commit:gemlockfile push npublish",
    "npublish": "npm publish",
    "jest:update": "env DEBUG=scrolex:* NODE_ENV=test jest --updateSnapshot",
    "jest": "env DEBUG=scrolex:* NODE_ENV=test jest",
    "test:update": "npm-run-all build jest:update",
    "test:acceptance": "./scripts/test-acceptance.sh",
    "test:watcher": "npm test -- --watch",
    "test": "npm-run-all jest",
    "upgrade:modules": "npm-check-updates -u --semverLevel major && yarn",
    "web:install": "cd website && npm run install",
    "web:start": "cd website && npm run start"
  },
  "lanyon": {
    "statistics": "stats.html",
    "entries": [
      "app"
    ],
    "prerequisites": {
      "sh": {
        "preferred": "0.5.7",
        "range": ">=0.0.1",
        "writeShim": true
      },
      "node": {
        "preferred": "4.6.1",
        "range": ">=0.10"
      },
      "docker": {
        "preferred": "1.12.3",
        "range": ">=1.12"
      },
      "rbenv": {
        "preferred": "1.0.0",
        "range": ">=1"
      },
      "rvm": {
        "preferred": "1.27.0",
        "range": ">=1"
      },
      "ruby": {
        "preferred": "2.3.3",
        "range": ">=2.1.0",
        "writeShim": true
      },
      "gem": {
        "preferred": "2.0.14.1",
        "range": ">=2",
        "writeShim": true
      },
      "bundler": {
        "preferred": "1.13.0",
        "range": ">=1",
        "writeShim": true
      },
      "jekyll": {
        "preferred": "3.4.3",
        "range": ">=3",
        "writeShim": true
      }
    },
    "ports": {
      "assets": 3000,
      "content": 4000
    },
    "gems": {
      "github-pages": "164",
      "nokogiri": "1.8.0",
      "pkg-config": "1.1.7",
      "jekyll-lunr-js-search": "3.3.0",
      "stringex": "2.6.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvz/lanyon.git"
  },
  "keywords": [
    "jekyll",
    "static",
    "site",
    "generator"
  ],
  "author": "Kevin van Zonneveld",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kvz/lanyon/issues"
  },
  "homepage": "https://github.com/kvz/lanyon#readme",
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true
    }
  },
  "devDependencies": {
    "eslint": "4.9.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "jest": "21.2.1"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "async": "2.5.0",
    "autoprefixer": "7.1.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-proto-to-assign": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "bower-webpack-plugin": "0.1.9",
    "browser-sync": "2.18.13",
    "chalk": "2.2.0",
    "coffee-loader": "0.8.0",
    "coffee-script": "1.12.7",
    "coffeescript": "2.0.1",
    "common-tags": "1.4.0",
    "css-loader": "0.28.7",
    "depurar": "0.3.0",
    "es6-promise": "4.1.1",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "3.0.1",
    "fakefile": "0.0.9",
    "file-loader": "1.1.5",
    "font-awesome": "4.7.0",
    "font-awesome-loader": "1.0.2",
    "globby": "6.1.0",
    "http-server": "0.10.0",
    "imports-loader": "0.7.1",
    "js-yaml": "3.10.0",
    "less": "3.0.0-alpha.3",
    "less-loader": "4.0.5",
    "lodash": "4.17.4",
    "log-symbols": "2.1.0",
    "log-update": "2.1.0",
    "node-libs-browser": "2.0.0",
    "node-sass": "4.5.3",
    "nodemon": "1.12.1",
    "npm-check-updates": "2.13.0",
    "npm-run-all": "4.1.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "os-tmpdir": "1.0.2",
    "pad": "2.0.1",
    "parallelshell": "3.0.1",
    "postcss-loader": "2.0.8",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.1.1",
    "resolve-url-loader": "2.1.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "scrolex": "0.0.27",
    "semver": "5.4.1",
    "shelljs": "0.7.8",
    "spawn-sync": "1.0.15",
    "style-loader": "0.19.0",
    "uglify": "0.1.5",
    "uglify-js": "3.1.4",
    "uglify-loader": "2.0.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.9.2",
    "webpack-hot-middleware": "2.20.0",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-svgstore-plugin": "4.0.1",
    "webpack-visualizer-plugin": "0.1.11"
  }
}
